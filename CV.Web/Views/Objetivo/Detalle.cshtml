@model ObjetivoDTO

<div class="container wow fadeInUp">
    <div class="row">

        <div class="col-lg-12 col-sm-12 text-capitalize">
            <div class="col-sm-12">
                <a href="@Url.Action("Index", "Objetivo")"><p class="btn btn-lg btn-primary text-uppercase"><i class="fa fa-chevron-circle-left"></i> Volver</p></a>
                <a href="@Url.Action("Actualizar", "Objetivo", new { id = Model.ObjetivoId})"><p class="btn btn-outline-dark btn-lg text-uppercase"><i class="fa fa-edit"></i> Editar</p></a>
            </div>

            <div class="card col-lg-12 col-sm-12">
                <div class="card-header text-center">
                    <i class="fa fa-graduation-cap fa-2x text-primary"></i> Perfil Profesional: @Model.Descripcion
                </div>
            </div>
        </div>
    </div>
</div>